<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="https://music-encoding.org/schema/4.0.0/mei-all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="https://music-encoding.org/schema/4.0.0/mei-all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<mei xmlns="http://www.music-encoding.org/ns/mei" meiversion="4.0.0">
   <meiHead>
      <fileDesc>
         <titleStmt>
            <title>Rests and dotted rests</title>
         </titleStmt>
         <pubStmt>
            <date>2017-04-19</date>
         </pubStmt>
         <notesStmt>
            <annot>Verovio support rests from long to 256th.</annot>
         </notesStmt>
      </fileDesc>
      <encodingDesc>
         <appInfo>
            <application version="2.0.0" label="2">
               <name>Verovio</name>
            </application>
         </appInfo>
      </encodingDesc>
   </meiHead>
   <music>
      <body>
         <mdiv>
            <score>
               <scoreDef key.mode="major" key.sig="0">
                  <staffGrp n="1">
                     <staffDef n="1" lines="5" clef.shape="G" clef.line="2" />
                  </staffGrp>
               </scoreDef>
               <section>
                  <measure n="1">
                     <staff n="1">
                        <layer n="1">
                           <rest dur="long" />
                           <rest dur="breve" />
                           <rest dur="1" />
                           <rest dur="2" />
                           <rest dur="4" />
                           <rest dur="8" />
                           <rest dur="16" />
                           <rest dur="32" />
                           <rest dur="64" />
                           <rest dur="128" />
                           <rest dur="256" />
                        </layer>
                     </staff>
                  </measure>
                  <measure right="dbl" n="1">
                     <staff n="1">
                        <layer n="1">
                           <rest dots="1" dur="2" />
                           <rest dots="1" dur="4" />
                           <rest dots="1" dur="8" />
                           <rest dots="1" dur="16" />
                           <rest dots="1" dur="32" />
                           <rest dots="1" dur="64" />
                           <rest dots="1" dur="128" />
                           <rest dots="1" dur="256" />
                        </layer>
                     </staff>
                  </measure>
               </section>
            </score>
         </mdiv>
      </body>
   </music>
</mei>
